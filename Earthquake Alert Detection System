import java.util.*;

public class EarthquakeAlert
{
  public Static void main(String[]args)
    {
       Scanner sc = new Scanner(System.in);
       System.out.println("Enter Richter Scale Reading:");
       double r = sc.nextDouble();
       if(r<3)
       System.out.println("Minor Shake Safe");
       else if(r<6)
       System.out.println("Moderate Earthquake Stay Alert!");
       else
       System.out.println("Major Earthqauke Evaculate immediately!");
     }
}
